{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#cba6f7",
          "template": "{{ if .SSHSession }} {{ end }}"
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#89b4fa",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 3
          },
          "template": " {{ .Path }}"
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "#a6e3a1",
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .Working.Changed }}*{{ end }}",
          "properties": {
            "fetch_status": false,
            "fetch_upstream_icon": true
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "executiontime",
          "style": "plain",
          "foreground": "#f9e2af",
          "template": " {{ .FormattedMs }}",
          "properties": {
            "threshold": 500,
            "style": "austin"
          }
        },
        {
          "type": "command",
          "style": "plain",
          "foreground": "#f38ba8",
          "template": " {{ if gt .Code 0 }} {{ .Code }}{{ end }}",
          "properties": {
            "always_enabled": false
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#6c7086",
          "template": " {{ .CurrentDate | date .Format }}",
          "properties": {
            "time_format": "15:04"
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "python",
          "style": "plain",
          "foreground": "#74c7ec",
          "template": "{{ if .Venv }} {{ if .Full }}({{ .Venv }} {{ .Full }}){{ else }}({{ .Venv }}){{ end }}{{ end }}",
          "properties": {
            "home_enabled": false,
            "fetch_virtual_env": true,
            "display_mode": "context",
            "fetch_version": true
          }
        },
        {
          "type": "project",
          "style": "plain",
          "foreground": "#94e2d5",
          "template": "{{ if .Version }} {{ .Version }}{{ end }}"
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#a6e3a1",
          "template": "❯ "
        }
      ],
      "type": "prompt"
    }
  ],
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#a6e3a1",
    "template": "❯ "
  },
  "secondary_prompt": {
    "foreground": "#6c7086",
    "template": "▶ "
  },
  "version": 3
}
